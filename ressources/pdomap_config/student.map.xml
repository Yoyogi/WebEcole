<pdomap>
  <adapter name="students" table="etudiant">
    <entity>
      <properties>
        <primary name="id" column="id_etudiant" />
        <property name="name" column="nom" type="char" size="45" />
        <property name="surname" column="prenom" type="char" size="45" />
        <property name="birthday" column="date_naissance" type="date"/>
        <property name="adress" column="rue" type="char" size="45" />
        <property name="zipCode" column="cp" type="int" size="11" />
        <property name="city" column="ville" type="char" size="45" />
        <property name="mail" column="email" type="char" size="45" />
        <property name="login" column="ulogin" type="char" size="45" />
        <property name="passwd" column="passwd" type="char" size="45" />
        <property name="p33333hoto" column="photo" type="char" size="250" />
      </properties>
    </entity>
  </adapter>
</pdomap>